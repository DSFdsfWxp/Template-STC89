{
    // 控制是否根据文档中的文字计算自动完成列表。
    "editor.wordBasedSuggestions": "off",
    // tab键补全
    "editor.tabCompletion": "on",
    // 词语优先级
    "editor.suggest.localityBonus": true,
    // 这里更改为根据之前的补全进行排序
    "editor.suggestSelection": "recentlyUsed",
    "files.associations": {
        "*.jsm": "javascript",
        "*.json": "json",
        "*.frag": "glsl",
        "*.h": "c",
        "reg52.h": "c",
        "8052.h": "c",
        "RC522.C": "cpp",
        "type_traits": "cpp",
        "intrins.h": "c",
        "REG52.H": "cpp",
        "INTRINS.H": "cpp",
        "STDLIB.H": "cpp",
        "MATH.H": "cpp",
        "cmath": "c",
        "REGX52.H": "cpp",
        "STRING.H": "cpp",
        "REG51.H": "cpp",
        "cstdlib": "c"
    },
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.clang_format_fallbackStyle": "Webkit"
}